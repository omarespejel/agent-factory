{
  "output": {
    "filePath": "context/context.xml",
    "style": "xml",
    "removeComments": false,
    "removeEmptyLines": false,
    "showLineNumbers": true
  },
  "include": [
    "README.md",
    "AGENTS.md",
    "PLAN.md",
    ".gitignore",
    ".env.template",
    "config.toml",
    "mcp_config.json",
    "system_prompt.md",
    "launch.py",
    "Makefile",
    "docs/**",
    "prompts/**",
    "scripts/**",
    "project/Scarb.toml",
    "project/src/**",
    "project/tests/**"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "*.lock",
      "context*.xml",
      "context-*-*.xml",
      ".manager/**",
      "context/**",
      "node_modules/**",
      ".git/**",
      "__pycache__/**",
      "*.pyc",
      "*.pyo",
      "*.pyd",
      ".venv/**",
      "venv/**",
      ".cursor/**",
      "*.swp",
      "*.swo",
      ".DS_Store",
      "**/.DS_Store",
      ".env",
      ".env.local",
      ".env.*.local",
      "dist/**",
      "build/**",
      "*.egg-info/**",
      ".eggs/**",
      ".pytest_cache/**",
      ".coverage",
      ".coverage.*",
      "coverage.xml",
      "htmlcov/**",
      ".tox/**",
      ".hypothesis/**",
      ".ruff_cache/**",
      ".mypy_cache/**",
      ".cache/**",
      "*.log",
      "*.sqlite",
      "*.db",
      "logs/**",
      "project/target/**",
      "project/.scarb/**",
      "repomix.config.json.bak",
      "repomix.config.json.tmp"
    ]
  }
}
